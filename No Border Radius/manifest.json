{
    "manifest_version": 3,
    "name": "No Border Radius",
    "version": "6.9",
    "description": "Set border radius to 0px on all websites / Ustawia border radius na 0px na wszystkich stronach [AI generated]",
    "permissions": [
      "scripting",
      "storage",
      "activeTab"
    ],
    "host_permissions": [
      "<all_urls>"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }
  